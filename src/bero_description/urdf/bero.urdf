    <joint name="base_footprint_joint" type="fixed">
    <parent link="base_footprint"/>
    <child link="base_link"/>
    <origin xyz="0 0 0.048" rpy="0 0 0"/>
    </joint>
    
    <joint name="lower_joint" type="fixed">
        <parent link="base_link"/>
        <child link="lower_frame"/>
        <origin xyz="0.0 0.0 0.047" rpy="0.0 0.0 0.0"/>
    </joint>
    <joint name="plate_joint" type="fixed">
        <parent link="lower_frame"/>
        <child link="plate_link"/>
        <origin xyz="0.0 0.0 0.032" rpy="0.0 0.0 0.0"/>
    </joint>
    <joint name="upper_joint" type="fixed">
        <parent link="plate_link"/>
        <child link="upper_frame"/>
        <origin xyz="0.0 0.0 0.089" rpy="0.0 0.0 0.0"/>
    </joint>
    <joint name="top_joint" type="fixed">
        <parent link="upper_frame"/>
        <child link="upper_link"/>
        <origin xyz="0.0 0.0 0.0825" rpy="0.0 0.0 0.0"/>
    </joint>
    
    <joint name="laser_base_joint" type="fixed">
        <parent link="upper_link"/>
        <child link="laser_base_link"/>
        <origin xyz="0.03 0.0 0.041"/> 
    </joint>

    <joint name="laser_joint" type="fixed">
        <parent link="laser_base_link"/>
        <child link="laser"/>
        <origin xyz="0.0 0.0 0.0215" rpy="0.0 0.0 3.14159"/> 
    </joint>

    <joint name="imu_joint" type="fixed">
        <parent link="upper_link"/>
        <child link="imu_link"/>
        <origin xyz="0.0 0.0 0.0015" rpy="0 0 -1.5708"/>
    </joint>

    <joint name="pillar_joint_fr" type="fixed">
        <parent link="upper_link"/>
        <child link="laser_frame_fr"/>
        <origin xyz="0.055 -0.025 0.0145"/>
    </joint>
    <joint name="pillar_joint_fl" type="fixed">
        <parent link="upper_link"/>
        <child link="laser_frame_fl"/>
        <origin xyz="0.055 0.025 0.0145"/>
    </joint>
    <joint name="pillar_joint_br" type="fixed">
