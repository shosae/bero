    </joint>
    <joint name="wheel_joint_right" type="continuous">
        <parent link="base_link"/>
        <child link="wheel_link_right"/>
        <origin xyz="0.07325 -0.1269 -0.007" rpy="1.5708 0 0.5236"/>
